{
    "name": "klib",
    "libname": "libklib",
    "version": "0.2.0",
    "short_description": "hashtable / pool / string cache plugins",
    "long_description": "Container plugins. Very efficient hashtable, an int pool to manage resources, string cache to pass/store strings as integers",
    "csound_version": "6.14",
    "binaries": {
        "linux": {
            "url": "linux/libklib.so", 
            "build_platform": "Ubuntu 16.04"
        },
        "macos": {
            "url": "macos/libklib.dylib",
            "build_platform": "10.14.0"
        },
        "windows": {
            "url": "win32/libklib.dll",
            "build_platform": "Windows 10"
        }
    },
    "opcodes": [
        "dict_exists", "dict_free", "dict_get", "dict_iter", "dict_new", "dict_print",
        "dict_query", "dict_set", "dict_size", "pool_at", "pool_capacity", "pool_gen",
        "pool_new", "pool_pop", "pool_push", "pool_size",
        "strcache", "strview"
    ],
